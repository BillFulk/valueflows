#!/usr/bin/env node

var ghpages = require('gh-pages')
var join = require('path').join
 
ghpages.publish(
  join(__dirname, '../.build'),
  {
    add: true,
    repo: 'git@github.com:valueflows/valueflo.ws',
    branch: 'index-test'
  },
  function(err) {
    if (err) {
      console.error(err)
      return process.exit(1)
    }
    console.log("pushed")
  }
);
